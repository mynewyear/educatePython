from typing import List, Any

lst1 = [1, 3, 4, 5]
lst2 = [2, 6, 7, 8]


def mergeArrays(lst1, lst2):
    # Write your code here
    # return [number for number in lst1 if number%2 !=0]
    mergeAr = []
    for item in lst1:
        for item2 in lst2:
            if item <= item2:
                mergeAr.append(item)
            else:
                mergeAr.append(item2)
    return mergeAr
